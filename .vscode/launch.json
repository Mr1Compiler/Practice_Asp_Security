{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch WebApp_UnderTheHood",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/WebApp_UnderTheHood/bin/Debug/net8.0/WebApp_UnderTheHood.dll",
      "args": [],
      "cwd": "${workspaceFolder}/WebApp_UnderTheHood",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/WebApp_UnderTheHood/Views"
      }
    }
  ]
}